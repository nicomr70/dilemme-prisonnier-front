/div>\n}\n\n","/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/CreateGame/CreateGame.jsx",[],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/ResumeGame/ResumeGame.jsx",[],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/Game/Game.jsx",["74","75"],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/Home/Home.jsx",[],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/Play/Play.jsx",["76","77","78","79","80"],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/WaitLastPlayer/WaitLastPlayer.jsx",["81","82","83","84"],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/FinDePartie/FinDePartie.jsx",["85","86"],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/viewGame/ViewGame.jsx",["87"],"/Users/nico/Documents/fac/M1 MIAGE/S7/PC/DP/dilemme-prisonnier-front/src/component/PageNotFound/PageNotFound.jsx",["88","89"],"import {useNavigate, useParams} from \"react-router-dom\";\n\n\nexport function PageNotFound(){\n    const navigate = useNavigate();\n    const handleClick = ()=>{\n            navigate(\"/\")\n    }\n\n    return <div className=\"page404\">\n        <div >\n            <img src=\"https://i2.wp.com/tomrogerswebdesign.com/wp-content/uploads/2016/02/doh-homer-computer500x295.jpg?ssl=1\"/>\n        </div>\n        <div>\n            <p>D'hoo cette page n'exite pas , retourne à l'accueil</p>\n            <button onClick={handleClick}>retour au menu</button>\n        </div>\n    </div>\n}",{"ruleId":"90","severity":1,"message":"91","line":1,"column":16,"nodeType":"92","messageId":"93","endLine":1,"endColumn":25},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"90","severity":1,"message":"98","line":3,"column":9,"nodeType":"92","messageId":"93","endLine":3,"endColumn":20},{"